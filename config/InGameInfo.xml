    <lines at="bottomleft">
        <line>
            <var>equippedicon</var>
            <str> </str>
            <op>
                <str>ge</str>
                <pct>
                    <var>equippeddamageleft</var>
                    <var>equippedmaxdamage</var>
                </pct>
                <num>50</num>
                <num>20</num>
                <num>10</num>
                <num>0</num>
                <str>$e</str>
                <str>$6</str>
                <str>$c</str>
                <str>$4</str>
                <str/>
            </op>
            <if>
                <equal>
                    <var>equippeduniquename</var>
                    <str>teloaddon:Chainsaw</str>
                </equal>
                <concat>
                    <sub><var>equippeddamageleft</var><num>1000001</num></sub>
                    <str>$f / $e</str>
                    <sub><var>equippedmaxdamage</var><num>1000001</num></sub>
                </concat>
                <max>
                    <var>equippeddamageleft</var>
                    <num>0.5</num>
                    <concat>
                        <sub><var>equippeddamageleft</var><num>1</num></sub>
                        <str>$f / $e</str>
                        <sub><var>equippedmaxdamage</var><num>1</num></sub>
                    </concat>
                    <var>equippedname</var>
                </max>
            </if>
        </line>
        <line>
            <str> </str>
        </line>
    </lines>
